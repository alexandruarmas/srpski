<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Background -->
  <rect width="300" height="300" fill="#f5f5f5"/>
  
  <!-- Wooden board instead of plate -->
  <rect x="50" y="170" width="200" height="40" rx="5" ry="5" fill="#8b4513"/>
  <rect x="60" y="175" width="180" height="30" rx="3" ry="3" fill="#a0522d"/>
  <line x1="80" y1="175" x2="80" y2="205" stroke="#8b4513" stroke-width="1"/>
  <line x1="130" y1="175" x2="130" y2="205" stroke="#8b4513" stroke-width="1"/>
  <line x1="180" y1="175" x2="180" y2="205" stroke="#8b4513" stroke-width="1"/>
  <line x1="220" y1="175" x2="220" y2="205" stroke="#8b4513" stroke-width="1"/>
  
  <!-- Lepinja (Flatbread) -->
  <ellipse cx="150" cy="165" rx="90" ry="25" fill="#e6d2a8"/>
  
  <!-- Sarplaninska Pljeskavica (thicker, mountain-style) -->
  <ellipse cx="150" cy="155" rx="70" ry="20" fill="#654321"/>
  <ellipse cx="150" cy="145" rx="65" ry="15" fill="#8b4513"/>
  
  <!-- Mountain herb/spice mix visible on top -->
  <circle cx="130" cy="140" r="2" fill="#355e3b"/>
  <circle cx="150" cy="138" r="2" fill="#355e3b"/>
  <circle cx="170" cy="140" r="2" fill="#355e3b"/>
  <circle cx="140" cy="130" r="1" fill="#355e3b"/>
  <circle cx="160" cy="130" r="1" fill="#355e3b"/>
  
  <!-- Small mountain design in background -->
  <path d="M 70,90 L 100,40 L 130,80 L 170,30 L 210,70 L 230,50 Z" fill="#e0e0e0" opacity="0.3"/>
  
  <!-- Text Label -->
  <text x="150" y="250" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#333">
    Å arplaninska Pljeskavica
  </text>
</svg>
