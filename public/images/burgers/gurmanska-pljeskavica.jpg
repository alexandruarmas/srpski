<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Background -->
  <rect width="300" height="300" fill="#f5f5f5"/>
  
  <!-- Plate -->
  <ellipse cx="150" cy="180" rx="120" ry="40" fill="#e0e0e0"/>
  
  <!-- Lepinja (Flatbread) -->
  <ellipse cx="150" cy="170" rx="90" ry="25" fill="#f9e8c7"/>
  
  <!-- Pljeskavica (Meat patty) layers -->
  <ellipse cx="150" cy="160" rx="80" ry="15" fill="#8b4513"/>
  <ellipse cx="150" cy="150" rx="75" ry="10" fill="#a64b2a"/>
  
  <!-- Stuffing (cheese and ham visible) -->
  <ellipse cx="150" cy="150" rx="40" ry="5" fill="#f9d887"/>
  <rect x="120" y="145" width="60" height="5" fill="#e5a49e"/>
  
  <!-- Spices (paprika, etc) -->
  <circle cx="125" cy="145" r="2" fill="#d13c3c"/>
  <circle cx="135" cy="148" r="1" fill="#d13c3c"/>
  <circle cx="165" cy="146" r="2" fill="#d13c3c"/>
  <circle cx="175" cy="147" r="1" fill="#d13c3c"/>
  
  <!-- Text Label -->
  <text x="150" y="250" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#333">
    Gurmanska Pljeskavica
  </text>
</svg>
