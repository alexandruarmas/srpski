<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Background -->
  <rect width="300" height="300" fill="#f5f5f5"/>
  
  <!-- Small bowl -->
  <ellipse cx="150" cy="180" rx="70" ry="30" fill="#e0e0e0"/>
  <ellipse cx="150" cy="180" rx="60" ry="25" fill="#d5d5d5"/>
  
  <!-- Ajvar (red pepper spread) -->
  <ellipse cx="150" cy="175" rx="50" ry="20" fill="#c43b24"/>
  <ellipse cx="150" cy="173" rx="45" ry="17" fill="#d13c3c"/>
  
  <!-- Texture details and oil on top -->
  <path d="M 120,165 C 130,160 140,163 150,160 C 160,163 170,160 180,165" fill="none" stroke="#e25822" stroke-width="2"/>
  <path d="M 125,168 C 135,163 145,166 155,163 C 165,166 175,163 175,168" fill="none" stroke="#e25822" stroke-width="1.5"/>
  
  <!-- Oil spots -->
  <circle cx="130" cy="160" r="3" fill="#f0c264" opacity="0.7"/>
  <circle cx="150" cy="162" r="2" fill="#f0c264" opacity="0.7"/>
  <circle cx="170" cy="161" r="2.5" fill="#f0c264" opacity="0.7"/>
  
  <!-- Spoon with some ajvar -->
  <path d="M 80,170 C 90,160 110,160 120,170" fill="none" stroke="#c0c0c0" stroke-width="2"/>
  <ellipse cx="120" cy="170" rx="10" ry="5" fill="#c0c0c0"/>
  <ellipse cx="120" cy="168" rx="8" ry="3" fill="#d13c3c"/>
  
  <!-- Small pieces of red pepper visible -->
  <path d="M 140,170 L 145,165 L 150,170 L 145,175 Z" fill="#9a0000"/>
  <path d="M 160,165 L 165,160 L 170,165 L 165,170 Z" fill="#9a0000"/>
  
  <!-- Text Label -->
  <text x="150" y="250" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#333">
    Ajvar
  </text>
</svg>
