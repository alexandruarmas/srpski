<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Background -->
  <rect width="300" height="300" fill="#f5f5f5"/>
  
  <!-- Bowl -->
  <ellipse cx="150" cy="180" rx="90" ry="40" fill="#e0e0e0"/>
  <ellipse cx="150" cy="180" rx="80" ry="35" fill="#f0f0f0"/>
  
  <!-- Salad base -->
  <ellipse cx="150" cy="170" rx="70" ry="30" fill="#efffef"/>
  
  <!-- Tomatoes (red chunks) -->
  <circle cx="120" cy="160" r="10" fill="#e74c3c"/>
  <circle cx="135" cy="175" r="12" fill="#e74c3c"/>
  <circle cx="160" cy="165" r="8" fill="#e74c3c"/>
  <circle cx="180" cy="170" r="10" fill="#e74c3c"/>
  
  <!-- Cucumbers (green chunks) -->
  <rect x="115" y="150" width="15" height="8" rx="4" ry="4" fill="#2ecc71"/>
  <rect x="140" y="155" width="20" height="8" rx="4" ry="4" fill="#2ecc71"/>
  <rect x="165" y="160" width="15" height="8" rx="4" ry="4" fill="#2ecc71"/>
  
  <!-- Onions (white circles) -->
  <path d="M 125,145 A 10,5 0 0 0 145,145" fill="none" stroke="#f9f9f9" stroke-width="2"/>
  <path d="M 155,150 A 12,6 0 0 0 175,150" fill="none" stroke="#f9f9f9" stroke-width="2"/>
  <path d="M 125,160 A 8,4 0 0 0 140,160" fill="none" stroke="#f9f9f9" stroke-width="2"/>
  
  <!-- Serbian cheese (white crumbles on top) -->
  <circle cx="130" cy="140" r="3" fill="#ffffff"/>
  <circle cx="140" cy="145" r="4" fill="#ffffff"/>
  <circle cx="150" cy="140" r="3" fill="#ffffff"/>
  <circle cx="160" cy="143" r="2" fill="#ffffff"/>
  <circle cx="170" cy="145" r="3" fill="#ffffff"/>
  <circle cx="125" cy="150" r="2" fill="#ffffff"/>
  <circle cx="155" cy="150" r="2" fill="#ffffff"/>
  <circle cx="175" cy="150" r="2" fill="#ffffff"/>
  
  <!-- Text Label -->
  <text x="150" y="250" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#333">
    Å opska Salad
  </text>
</svg>
